$("ul.errors").html("")
<% if @frai.errors.any? %>
  <% @frai.errors.full_messages.each do |message| %>
$("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
<% end%>
<% else %>

$(".frais-index").html("<%= escape_javascript(render(:partial=>'index_frai_achat')) %>")
